{
  "name": "minado",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm run watch:css && node src/server.js & esbuild --watch --splitting --bundle ./src/App.jsx --outdir=./public/js --format=esm --target=esnext --define:process.env.NODE_ENV='\"development\"'",
    "build": "npm run watch:css && tsc && esbuild --minify --splitting --bundle ./src/App.jsx --outdir=./public/js --format=esm --target=esnext --define:process.env.NODE_ENV='\"production\"'",
    "dev": "npm run build && npm run start",
    "coverage": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --coverage",
    "test": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --watch",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/output.css",
    "clear_jest": "jest --clearCache"
  },
  "keywords": [],
  "author": "O(1) labs",
  "license": "Apache-2.0",
  "dependencies": {
    "esbuild": "^0.14.2",
    "experimental-zkapp-offchain-storage": "^0.2.0",
    "headlessui": "^0.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jsx": "^1.0.0",
    "snarkyjs": "^0.7.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.7.3",
    "xmlhttprequest-ts": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.23",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.3.1",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "webpack": "^5.74.0"
  }
}
